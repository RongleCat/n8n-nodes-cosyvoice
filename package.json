{
  "name": "n8n-nodes-cosyvoice",
  "version": "1.0.0",
  "description": "Aliyun CosyVoice TTS Node for n8n - 文字转语音节点",
  "license": "MIT",
  "homepage": "https://github.com/ronglecat/n8n-nodes-cosyvoice",
  "keywords": [
    "n8n-community-node-package",
    "cosyvoice",
    "tts",
    "text-to-speech",
    "aliyun",
    "dashscope",
    "chinese",
    "voice"
  ],
  "author": {
    "name": "ronglecat",
    "email": "ronglecat@163.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ronglecat/n8n-nodes-cosyvoice.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": false,
    "credentials": [
      "dist/credentials/CosyVoiceApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CosyVoice/CosyVoice.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "@types/node": "^25.0.3",
    "@types/ws": "^8.18.1",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
